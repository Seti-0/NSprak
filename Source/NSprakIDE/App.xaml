<Application x:Class="NSprakIDE.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:NSprakIDE"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <!-- Fluent Ribbon theme -->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/General.xaml"/>
                <ResourceDictionary Source="/Fluent;Component/Themes/Generic.xaml"/>
                <!--<ResourceDictionary Source="Themes/RibbonTheme.xaml"/>-->
                <ResourceDictionary Source="Icons/IconDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>

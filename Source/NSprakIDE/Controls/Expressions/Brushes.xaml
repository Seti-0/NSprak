<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:NSprakIDE.Controls.Expressions">

    <SolidColorBrush x:Key="{x:Static local:TreeColorNames.KeySymbol}" Color="#FF5B5A5D"/>
    <SolidColorBrush x:Key="{x:Static local:TreeColorNames.Keyword}" Color="#FF0A9E10"/>
    <SolidColorBrush x:Key="{x:Static local:TreeColorNames.Type}" Color="#FF0A9E10"/>
    <SolidColorBrush x:Key="{x:Static local:TreeColorNames.Name}" Color="#FFF75A37"/>
    <SolidColorBrush x:Key="{x:Static local:TreeColorNames.Literal}" Color="#FF848289"/>
    <SolidColorBrush x:Key="{x:Static local:TreeColorNames.Comment}" Color="#FF357CF7"/>
    <SolidColorBrush x:Key="{x:Static local:TreeColorNames.Operator}" Color="#FF793BFF"/>
    <SolidColorBrush x:Key="{x:Static local:TreeColorNames.Debug}" Color="#FF848289"/>

</ResourceDictionary>